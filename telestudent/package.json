{
  "name": "telestudent",
  "version": "1.0.0",
  "description": "Telestudent, Powered by Relay Fullstack",
  "build": {
    "appId": "com.example.telestudent",
    "files": [
      "client/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "mac": {
      "icon": "src/assets/jivelogo.icns"
    },
    "win": {
      "icon": "src/assets/app.ico"
    }
  },
  "scripts": {
    "postinstall": "install-app-deps",
    "start": "./node_modules/.bin/babel-node server/index.js",
    "deploy": "cross-env NODE_ENV=production webpack --config webpack.config.js  && cross-env NODE_ENV=production node ./build/index.js",
    "lint": "eslint --ignore-path .gitignore client server",
    "heroku-postbuild": "cross-env NODE_ENV=production webpack --config webpack.config.js && cross-env NODE_ENV=production ./node_modules/.bin/babel ./server --out-dir ./lib",
    "electron": "electron .",
    "dev": "nf start -p 3000",
    "release": "build"
  },
  "pre-commit": "lint",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lvarayut/relay-fullstack.git"
  },
  "engines": {
    "node": "6.6.0",
    "npm": "3.10.8"
  },
  "keywords": [
    "react",
    "relay",
    "graphql",
    "express",
    "es6",
    "es7",
    "jsx",
    "webpack",
    "babel",
    "material design lite",
    "postcss",
    "scaffolding",
    "fullstack"
  ],
  "author": "Varayut Lerdkanlayanawat",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lvarayut/relay-fullstack/issues"
  },
  "homepage": "https://github.com/lvarayut/relay-fullstack",
  "main": "client/electron-starter.js",
  "devDependencies": {
    "eslint": "^3.8.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "nodemon": "^1.11.0",
    "pre-commit": "^1.1.3",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.3.2",
    "electron": "^1.7.9",
    "electron-builder": "^19.45.4"
  },
  "dependencies": {
    "amazon-cognito-identity-js": "^1.24.0",
    "autoprefixer": "^6.5.1",
    "aws-api-gateway-client": "^0.2.9",
    "aws-sdk": "^2.149.0",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-relay-plugin": "^0.9.3",
    "chalk": "^1.1.3",
    "connect-history-api-fallback": "^1.3.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "express": "^4.14.0",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^0.9.0",
    "foreman": "^2.0.0",
    "fs": "0.0.1-security",
    "html-webpack-plugin": "^2.22.0",
    "jquery": "^3.2.1",
    "json-loader": "^0.5.7",
    "localforage": "^1.5.3",
    "lodash": "^4.16.4",
    "material-ui": "^1.0.0-beta.21",
    "material-ui-icons": "^1.0.0-beta.17",
    "nedb": "^1.8.0",
    "normalize.css": "^5.0.0",
    "path": "^0.12.7",
    "postcss-loader": "^1.0.0",
    "precss": "^1.4.0",
    "prop-types": "^15.6.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-dropdown": "^1.1.0",
    "react-mdl": "^1.7.2",
    "react-relay": "^0.9.3",
    "react-router": "^2.8.1",
    "react-router-relay": "^0.13.5",
    "react-tex": "^1.3.0",
    "style-loader": "^0.13.1",
    "toastr": "^2.1.2",
    "url": "^0.11.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  }
}
